{
  "name": "plume",
  "displayName": "Plume",
  "description": "Plume language support for Visual Studio Code",
  "version": "0.1.1",
  "icon": "images/icon.png",
  "publisher": "PlumeLang",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters",
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Inerska/vscode-plume"
  },
  "contributes": {
    "languages": [{
      "id": "plume",
      "aliases": ["Plume", "plume"],
      "extensions": [".plm"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./images/file_icon.png",
        "light": "./images/file_icon.png"
      }
    }],
    "grammars": [{
      "language": "plume",
      "scopeName": "source.plume",
      "path": "./syntaxes/plume.tmLanguage.json"
    }],
    "snippets": [{
      "language": "plume",
      "path": "./snippets/snippets.json"
    }]
  }
}
